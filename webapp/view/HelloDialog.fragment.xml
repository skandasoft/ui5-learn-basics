<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
	<Dialog
		contentHeight=""
		contentWidth=""
		draggable="false"
		escapeHandler=""
		horizontalScrolling="true"
		icon=""
		resizable="false"
		showHeader="true"
		state="None"
		stretch="false"
		title="{i18n>openDialogButtonTitle}"
		type="Standard"
		verticalScrolling="true"
		id="helloDialog"
	>
		<beginButton>
			<Button
				activeIcon=""
				enabled="true"
				icon="sap-icon://close-command-field"
				iconDensityAware="true"
				iconFirst="true"
				text="{i18n>closeDialogButtonText}"
				textDirection="Inherit"
				type="Default"
				width=""
				press=".onCloseDialog"
			/>
		</beginButton>
		<buttons>
		</buttons>
		<content>
			<c:Icon
				activeBackgroundColor=""
				activeColor=""
				alt=""
				backgroundColor=""
				color=""
				decorative="true"
				height=""
				hoverBackgroundColor=""
				hoverColor=""
				noTabStop="false"
				size="8rem"
				src="sap-icon://hello-world"
				useIconTooltip="true"
				width=""
				class="sapUiMediumMargin"
			/>
		</content>
		<customHeader>
		</customHeader>
		<endButton>
		</endButton>
		<subHeader>
		</subHeader>
	</Dialog>
</c:FragmentDefinition>